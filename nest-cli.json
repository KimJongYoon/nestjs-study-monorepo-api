{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "projects": {
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "api-usin": {
      "type": "application",
      "root": "apps/api-usin",
      "entryFile": "main",
      "sourceRoot": "apps/api-usin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-usin/tsconfig.app.json"
      }
    },
    "service-auth": {
      "type": "application",
      "root": "apps/service-auth",
      "entryFile": "main",
      "sourceRoot": "apps/service-auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/service-auth/tsconfig.app.json"
      }
    },
    "service-user": {
      "type": "application",
      "root": "apps/service-user",
      "entryFile": "main",
      "sourceRoot": "apps/service-user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/service-user/tsconfig.app.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "service-admin-account": {
      "type": "application",
      "root": "apps/service-admin-account",
      "entryFile": "main",
      "sourceRoot": "apps/service-admin-account/src",
      "compilerOptions": {
        "tsConfigPath": "apps/service-admin-account/tsconfig.app.json"
      }
    },
    "api-admin": {
      "type": "application",
      "root": "apps/api-admin",
      "entryFile": "main",
      "sourceRoot": "apps/api-admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-admin/tsconfig.app.json"
      }
    },
    "service-post": {
      "type": "application",
      "root": "apps/service-post",
      "entryFile": "main",
      "sourceRoot": "apps/service-post/src",
      "compilerOptions": {
        "tsConfigPath": "apps/service-post/tsconfig.app.json"
      }
    }
  },
  "compilerOptions": {
    "assets": [
      "**/*.proto"
    ],
    "watchAssets": true,
    "webpack": true
  }
}